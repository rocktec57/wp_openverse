{
  "$schema": "http://json.schemastore.org/swcrc",
  "module": {
    // `commonjs` required to support `export =`, which ESLint expects from configs
    "type": "commonjs"
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    }
  }
}
